<?xml version="1.0" ?>
<control xmlns="http://www.sap.com/sap.ui.library.xsd" >

	<name>CustomListItem</name>
	<author>SAP AG</author>
	<documentation>sap.m.CustomListItem Control</documentation>

	<baseType>ListItemBase</baseType>
	<aggregations default="content">
		<aggregation cardinality="0..n" name="content" type="sap.ui.core/Control" bindable="bindable">
			<documentation>The content of this list item</documentation>
		</aggregation>
	</aggregations>
</control>
